{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 9002",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint .",
    "postinstall": "prisma generate",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:jobs": "jest src/app/admin/jobs",
    "test:api": "jest src/app/api/jobs",
    "test:validation": "jest src/lib/validation",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=__tests__ --testPathIgnorePatterns=integration",
    "db:seed": "ts-node --transpile-only --project tsconfig.scripts.json -r tsconfig-paths/register src/scripts/seed-database.ts",
    "db:seed:full": "ts-node --transpile-only --project tsconfig.scripts.json -r tsconfig-paths/register src/scripts/run-seed.ts --reset --sync --validate",
    "db:migrate": "ts-node --transpile-only --project tsconfig.scripts.json -r tsconfig-paths/register src/scripts/migrate-json-data.ts",
    "db:reset": "npx prisma db push --force-reset && npm run db:seed",
    "db:sync": "ts-node --transpile-only --project tsconfig.scripts.json -r tsconfig-paths/register -e \"const { ContentSyncService } = require('./src/lib/services/content-sync-service'); ContentSyncService.syncAllJsonToDatabase().then(() => process.exit(0))\"",
    "db:validate": "ts-node --transpile-only --project tsconfig.scripts.json -r tsconfig-paths/register -e \"const { ContentSyncService } = require('./src/lib/services/content-sync-service'); ContentSyncService.validateDataConsistency().then(r => { console.log(r.consistent ? '✅ Consistent' : '❌ Issues:', r.issues); process.exit(0) })\"",
    "db:test": "ts-node --transpile-only --project tsconfig.scripts.json -r tsconfig-paths/register src/scripts/test-seed.ts",
    "db:seed:employees": "ts-node --transpile-only --project tsconfig.scripts.json -r tsconfig-paths/register src/scripts/seed-employees.ts",
    "seed": "ts-node --transpile-only --project tsconfig.scripts.json -r tsconfig-paths/register src/scripts/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.475.0",
    "next": "16.1.6",
    "nodemailer": "^8.0.1",
    "prisma": "^6.16.2",
    "react": "19.2.4",
    "react-day-picker": "9.13.0",
    "react-dom": "19.2.4",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.9",
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3",
    "babel-jest": "^30.1.2",
    "eslint": "9.36.0",
    "eslint-config-next": "16.1.6",
    "genkit-cli": "^1.14.1",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  }
}
